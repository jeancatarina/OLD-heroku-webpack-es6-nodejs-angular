{
	"name": "AngularJSTree",
	"version": "0.0.1",
	"description": "Angular Tree",
	"main": "index.html",
	"scripts": {
		"babel:compile": "babel client --out-dir dist",
		"build-dev": "rimraf dist && npm run babel:compile && webpack --config webpack.dev.js",
		"build": "rimraf dist && npm run babel:compile && webpack --config webpack.prod.js -p",
		"dev": "webpack --watch --config webpack.dev.js",
		"start": "node server.js"
	},
	"dependencies": {
		"angular": "1.6.4",
		"angular-animate": "1.5.7",
		"angular-ui-router": "0.3.1",
		"bootstrap-css-only": "3.3.6",
		"express": "^4.17.1",
		"lodash": "4.13.1",
		"normalize.css": "4.1.1"
	},
	"devDependencies": {
		"angular-mocks": "1.5.7",
		"babel-cli": "^6.26.0",
		"babel-core": "6.7.6",
		"babel-eslint": "8.0.0",
		"babel-loader": "7.0.0",
		"babel-plugin-es6-promise": "1.1.1",
		"babel-plugin-syntax-dynamic-import": "6.18.0",
		"babel-plugin-transform-array-from": "1.0.0",
		"babel-plugin-transform-object-assign": "6.8.0",
		"babel-plugin-transform-react-jsx-source": "6.22.0",
		"babel-preset-airbnb": "1.1.1",
		"babel-preset-es2015": "6.6.0",
		"css-loader": "^3.2.0",
		"eslint": "^4.19.1",
		"file-loader": "0.9.0",
		"karma": "1.1.0",
		"raw-loader": "^3.1.0",
		"rimraf": "^3.0.0",
		"sass-loader": "^8.0.0",
		"style-loader": "^1.0.0",
		"url-loader": "0.5.7",
		"webpack": "4.29.6",
		"webpack-cli": "3.3.0",
		"webpack-config": "4.0.0",
		"webpack-merge": "^4.2.2",
		"webpack-stats-plugin": "0.2.1"
	},
	"keywords": [
		"angular",
		"webpack",
		"es6"
	],
	"author": "Jean Catarina",
	"license": "MIT",
	"babel": {
		"presets": [
			"es2015"
		]
	}
}
